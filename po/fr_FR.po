#: txt2regex.sh:105
msgid "usage: txt2regex [--nocolor|--whitebg] [--all|--prog PROGRAMS]"
msgstr "usage : txt2regex [--nocolor|--whitebg] [--all|--prog PROGRAMMES]"

#: txt2regex.sh:106
msgid "       txt2regex --showmeta"
msgstr "        txt2regex --showmeta"

#: txt2regex.sh:107
msgid "       txt2regex --showinfo PROGRAM [--nocolor]"
msgstr "        txt2regex --showinfo PROGRAMME [--nocolor]"

#: txt2regex.sh:108
msgid "       txt2regex --history VALUE [--all|--prog PROGRAMS]"
msgstr "        txt2regex --history VALEUR [--all|--prog PROGRAMMES]"

#: txt2regex.sh:109
msgid "       txt2regex --make LABEL [--all|--prog PROGRAMS]"
msgstr "        txt2regex --make NOM [--all|--prog PROGRAMMES]"

#: txt2regex.sh:111
msgid "OPTIONS"
msgstr "OPTIONS"

#: txt2regex.sh:113
#, fuzzy
msgid "  --all               Select all the available programs"
msgstr "  --all                 Utiliser tous les programmes supportés"

#: txt2regex.sh:114
msgid "  --nocolor           Do not use colors"
msgstr "  --nocolor             Ne pas utiliser de couleur"

#: txt2regex.sh:115
msgid "  --whitebg           Adjust colors for white background terminals"
msgstr "  --whitebg             Ajuster les couleurs pour les terminaux à fond clair"

#: txt2regex.sh:116
msgid "  --prog PROGRAMS     Specify which programs to use, separated by commas"
msgstr ""
"  --prog PROGRAMMES     Programmes à utiliser (liste de programmes séparés par\n"
"                        une virgule)"

#: txt2regex.sh:118
msgid "  --showmeta          Print a metacharacters table featuring all the programs"
msgstr ""
"  --showmeta            Afficher la table des métacaractères pour chaque\n"
"                        programme"

#: txt2regex.sh:119
msgid "  --showinfo PROGRAM  Print regex-related info about the specified program"
msgstr "  --showinfo PROGRAMME  Afficher des informations relatives à ce programme"

#: txt2regex.sh:120
msgid "  --history VALUE     Print a regex from the given history data"
msgstr "  --history VALEUR      Afficher sur STDOUT une regex déjà utilisée auparavant"

#: txt2regex.sh:121
#, fuzzy
msgid "  --make LABEL        Print a ready regex for the specified label"
msgstr "  --make NOM            Afficher la regex correspondant à cette présélection"

#: txt2regex.sh:123
msgid "  -V, --version       Print the program version and quit"
msgstr "  -V, --version         Afficher le nom et la version du logiciel puis quitter"

#: txt2regex.sh:124
msgid "  -h, --help          Print the help message and quit"
msgstr "  -h, --help            Afficher l'aide puis quitter"

