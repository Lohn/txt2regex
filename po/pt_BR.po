#: txt2regex.sh:88
msgid "ERROR"
msgstr "ERRO"

#: txt2regex.sh:105
msgid "usage: txt2regex [--nocolor|--whitebg] [--all|--prog PROGRAMS]"
msgstr "uso:   txt2regex [--nocolor|--whitebg] [--all|--prog PROGRAMAS]"

#: txt2regex.sh:106
msgid "       txt2regex --showmeta"
msgstr "       txt2regex --showmeta"

#: txt2regex.sh:107
msgid "       txt2regex --showinfo PROGRAM [--nocolor]"
msgstr "       txt2regex --showinfo PROGRAMA [--nocolor]"

#: txt2regex.sh:108
msgid "       txt2regex --history VALUE [--all|--prog PROGRAMS]"
msgstr "       txt2regex --history VALOR [--all|--prog PROGRAMAS]"

#: txt2regex.sh:109
msgid "       txt2regex --make LABEL [--all|--prog PROGRAMS]"
msgstr "       txt2regex --make NOME [--all|--prog PROGRAMAS]"

#: txt2regex.sh:111
msgid "OPTIONS:"
msgstr "OPÇÕES:"

#: txt2regex.sh:113
msgid "  --all               Select all the available programs"
msgstr "  --all               Ativa todos os programas disponíveis"

#: txt2regex.sh:114
msgid "  --nocolor           Do not use colors"
msgstr "  --nocolor           Não usar cores"

#: txt2regex.sh:115
msgid "  --whitebg           Adjust colors for white background terminals"
msgstr "  --whitebg           Ajuste as cores para terminais de fundo branco"

#: txt2regex.sh:116
msgid "  --prog PROGRAMS     Specify which programs to use, separated by commas"
msgstr "  --prog PROGRAMAS    Informe quais programas usar, separados por vírgulas"

#: txt2regex.sh:118
msgid "  --showmeta          Print a metacharacters table featuring all the programs"
msgstr "  --showmeta          Mostre uma tabela com os metacaracteres de todos os programas"

#: txt2regex.sh:119
msgid "  --showinfo PROGRAM  Print regex-related info about the specified program"
msgstr "  --showinfo PROGRAMA Mostra informações sobre o programa informado"

#: txt2regex.sh:120
msgid "  --history VALUE     Print a regex from the given history data"
msgstr "  --history VALOR     Mostra uma regex a partir dos dados de histórico informados"

#: txt2regex.sh:121
msgid "  --make LABEL        Print a ready regex for the specified label"
msgstr "  --make NOME         Mostra uma regex já pronta"

#: txt2regex.sh:123
msgid "  -V, --version       Print the program version and quit"
msgstr "  -V, --version       Mostra a versão do programa e sai"

#: txt2regex.sh:124
msgid "  -h, --help          Print the help message and quit"
msgstr "  -h, --help          Mostra a mensagem de ajuda e sai"

